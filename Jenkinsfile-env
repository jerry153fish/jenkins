pipeline {
    agent any 

    environment {
        DB_ENGINE    = 'sqlite'
		GITHUB_CRED = credentials('github-jerry')
    }

    stages {
        stage('Build') {
            steps {
				println GITHUB_CRED.split(':')
				sh '''
					set +x
					echo ${GITHUB_CRED}
					scripts/env_print.sh
				''' 
            }
        }
    }
}
